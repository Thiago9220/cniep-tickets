# Vercel Postgres URLs (get these from Vercel Dashboard after creating a Postgres database)
# POSTGRES_PRISMA_URL is used for queries (with connection pooling)
POSTGRES_PRISMA_URL="postgres://username:password@host:port/database?sslmode=require&pgbouncer=true"

# POSTGRES_URL_NON_POOLING is used for migrations (direct connection)
POSTGRES_URL_NON_POOLING="postgres://username:password@host:port/database?sslmode=require"
